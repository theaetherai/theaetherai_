{
  "env": {
    "NODE_ENV": "production",
    "NEXT_PRIVATE_STANDALONE": "1",
    "NEXT_TELEMETRY_DISABLED": "1",
    "CI": "false",
    "PRISMA_ENGINE_TYPE": "dataproxy",
    "PRISMA_CLIENT_ENGINE_TYPE": "dataproxy",
    "NODE_OPTIONS": "--max-old-space-size=1024"
  },
  "functions": {
    "app/api/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    },
    "app/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}